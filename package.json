{
  "name": "insit",
  "version": "1.0.0",
  "packageManager": "pnpm@8.1.1",
  "author": "AbelardoRuecker",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "@chansee97/commitlint-config": "link:./packages/commitlint-config",
    "@chansee97/eslint-config-vue": "link:./packages/eslint-config-vue",
    "@chansee97/stylelint-config": "link:./packages/stylelint-config",
    "@commitlint/cli": "^17.6.5",
    "bumpp": "^9.1.0",
    "eslint": "^8.43.0",
    "lint-staged": "^13.2.1",
    "simple-git-hooks": "^2.8.1",
    "stylelint": "^15.8.0",
    "typescript": "^5.0.4"
  },
  "simple-git-hooks": {
    "commit-msg": "npx --no-install commitlint --edit $HUSKY_GIT_PARAMS ",
    "pre-commit": "pnpm lint-staged && git add ."
  },
  "lint-staged": {
    "*.{js,jsx,,mjs,ts,tsx,json,,md,yml}": [
      "eslint --fix"
    ],
    "*.{css,scss,less,html}": [
      "stylelint --fix"
    ],
    "*.vue": [
      "eslint --fix",
      "stylelint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OliverHauck/insit.git"
  },
  "bugs": {
    "url": "https://github.com/OliverHauck/insit.git/issues"
  }
}